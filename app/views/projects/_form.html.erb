<%#= form_for :projects, url: projects_path do |f| %>
<%= form_for @project, url: {action: "create"}, html: {class: "form-inline"} do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
